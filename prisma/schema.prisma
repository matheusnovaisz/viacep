// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model adress {
  cep String @id @unique
  logradouro String
  complemento String?
  bairro String
  localidade String
  uf String
  ibge String
  gia String?
  ddd String
  siafi String
}
